swiper{
  height: 350rpx;
}
swiper image{
  width: 100%;
  height: 100%;
}
.sudoku-list{
display: flex;
flex-wrap: wrap;
box-sizing: border-box;
border-left: 1rpx solid #eee;
border-top: 1rpx solid #eee;
}
.sudoku-item{
  width: 33.33%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-right: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
}
.sudoku-list image{
  width: 60rpx;
  height: 60rpx;
}
.sudoku-list text{
  margin-top: 10rpx;
  font-size: 24rpx;
}
.foot-item{
  display: flex;
  padding: 20rpx 10rpx;
  justify-content: space-around;
}
.foot-item image{
  width: 43%;
}
